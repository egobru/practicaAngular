{
  "name": "angular-board",
  "version": "0.0.0",
  "scripts": {
    "build:prod": "n",
    "build:pub": "ng build --prod --output-path docs --base-href https://academiabinaria.github.io/angular-board/",
    "e2e": "ng e2e",
    "http-server": "http-server ./dist/angular-board/ -c-1 -p4271 -a localhost -o",
    "lint": "ng lint",
